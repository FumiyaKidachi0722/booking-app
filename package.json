{
  "name": "booking-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack && pnpm build:docs",
    "start": "next start",
    "lint": "next lint --fix && (command -v prettier >/dev/null && prettier --write \"**/*.{js,jsx,ts,tsx,css,md,json,html}\" || echo 'prettier not installed, skipping formatting')",
    "lint:ci": "next lint",
    "format": "(command -v prettier >/dev/null && prettier --write . --ignore-path .prettierignore || echo 'prettier not installed, skipping formatting')",
    "test": "vitest run",
    "test:cov": "vitest run --coverage",
    "codegen:types": "openapi-typescript docs/swagger.yaml -o src/shared/types/generated/openapi.types.ts",
    "codegen:zod": "openapi-zod-client docs/swagger.yaml -o src/shared/types/generated/openapi.zod.ts --export-schemas",
    "codegen": "pnpm codegen:types && pnpm codegen:zod",
    "build:docs": "redocly build-docs docs/swagger.yaml -o public/redoc.html",
    "postinstall": "pnpm codegen && pnpm build:docs"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "firebase-admin": "^13.4.0",
    "lucide-react": "^0.541.0",
    "next": "15.5.0",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@redocly/cli": "^2.0.7",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@zodios/core": "^10.9.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "jsdom": "^26.1.0",
    "openapi-typescript": "^7.9.1",
    "openapi-zod-client": "^1.18.3",
    "postcss": "^8.5.6",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b"
}
